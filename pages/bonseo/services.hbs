{{> organisms/header/logo-inline}}


<section class="og-image-articles l-flex l-flex--direction-column--reverse l-grid-column--full">
    {{#each this.mock.products}}
        <article class="ml-image-article a-mar--minus
                        a-pad-40
                        l-position l-flex l-flex--align-end l-flex--justify-center">
            {{> atoms/image/background
                    modifier="a-image--cover"
                    pictureClass=""
                    src=this.image}}
            {{> atoms/title/link title=this.name modifier="a-text--xl a-text--brand a-text--blur a-text--bold a-text--secondary"}}
        </article>
    {{/each}}
</section>

{{> organisms/list/category modifier="sky"}}
{{> organisms/list/category modifier="diable is-reverse"}}


{{> organisms/list/title-vertical
        modifier="is-reverse"
        navModifier="a-mar bs_viewport a-mi a-mi--temporal--left"}}
{{> organisms/list/title-vertical
        modifier="diable"
        navModifier="bs_viewport a-mi a-mi--temporal--left"}}
{{> organisms/footer/classic
        linkModifier="a-text--xs a-text--secondary a-pad--top-5 a-text--link--secondary"
        columnModifier="l-column l-column--1-4 l-column--mobile--1-1
                         a-pad--x-20 a-pad--mobile-40
                         a-border a-border--light a-border--left a-border--mobile--bottom"}}
