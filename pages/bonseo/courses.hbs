{{> organisms/header/logo-inline}}

<h1>Prueba</h1>
{{#each this.mock.products}}
    <h2>name: {{this.name}}</h2>
{{/each}}

<h1>--- Fin prueba</h1>

{{> molecules/breadcrumb/solid
        path=(arr "Inicio, Cursos")
        url=(arr "/bonseo/, /bonseo/cursos/")
        modifier="a-bg--secondary bs_theme"
        linksModifier=""
}}

{{> organisms/block/parallax-action}}

<section class="og-card-road l-flex l-flex--direction-column l-flex--justify-center">
    {{> atoms/title/h2 title="TÃ­tulo" modifier="a-pad a-text  a-text--center a-text--brand"}}
    <div class="l-flex l-flex--justify-center l-flex--wrap">
        {{#each this.mock.products}}
            {{> molecules/card/road
                    description=this.description
                    src=this.image
                    title=this.name }}
        {{/each}}
    </div>
</section>


{{> atoms/image/exposure
        src=images.product
        pictureClass=""
        modifier="a-image--cover"}}

{{> atoms/image/parallax image=images.product modifier="a-mar"}}




