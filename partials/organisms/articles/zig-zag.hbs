<section class="og-articles-zigzag a-pad-20 {{modifier}}">
    {{> atoms/title/h2
            title="Últimos Artículos"
            modifier="a-text--center a-text--bold a-pad"
    }}
    {{#each this.mock.products}}
        {{> molecules/article/extract
                title=this.title
                excerpt=this.title
                cta="Leer Más"
                src=this.image
                modifier="a-mi a-mi--left bs_viewport"
        }}
    {{/each}}
</section>
